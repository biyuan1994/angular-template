{
  "name": "comsunny-operate",
  "version": "0.0.1",
  "author": "jiayi",
  "description": "这是Comsunny运营管理平台",
  "keywords": [
    "Angular4",
    "admin",
    "pro",
    "ng4",
    "rxjs",
    "angular"
  ],
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --open --port 8000",
    "build": "ng build --env=prod --prod --aot --stats-json",
    "test": "npm run lint && ng test --watch",
    "watch": "ng test --browsers ChromeHeadless --reporters spec",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "analyzer": "webpack-bundle-analyzer dist/stats.json",
    "release": "standard-version && git push --follow-tags origin master"
  },
  "pre-commit": [
    "lint"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/jiayisheji/ng4-admin-pro.git"
  },
  "bugs": {
    "url": "https://github.com/jiayisheji/ng4-admin-pro/issues"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^4.4.6",
    "@angular/common": "^4.4.6",
    "@angular/compiler": "^4.4.6",
    "@angular/core": "^4.4.6",
    "@angular/flex-layout": "^2.0.0-beta.10-4905443",
    "@angular/forms": "^4.4.6",
    "@angular/http": "^4.4.6",
    "@angular/platform-browser": "^4.4.6",
    "@angular/platform-browser-dynamic": "^4.4.6",
    "@angular/router": "^4.4.6",
    "core-js": "^2.4.1",
    "rxjs": "^5.4.2",
    "zone.js": "^0.8.14"
  },
  "devDependencies": {
    "@angular/cli": "1.4.7",
    "@angular/compiler-cli": "^4.4.6",
    "@angular/language-service": "^4.4.6",
    "@types/jasmine": "~2.5.53",
    "@types/jasminewd2": "~2.0.2",
    "@types/node": "~6.0.60",
    "codelyzer": "~3.2.0",
    "ghooks": "^2.0.0",
    "jasmine-core": "~2.6.2",
    "jasmine-spec-reporter": "~4.1.0",
    "karma": "~1.7.0",
    "karma-chrome-launcher": "~2.1.1",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^1.2.1",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "pre-commit": "^1.2.2",
    "protractor": "~5.1.2",
    "standard-version": "^4.2.0",
    "ts-node": "~3.2.0",
    "tslint": "~5.7.0",
    "typescript": "~2.3.3",
    "validate-commit-msg": "^2.14.0",
    "webpack-bundle-analyzer": "^2.9.1"
  },
  "config": {
    "ghooks": {
      "commit-msg": "validate-commit-msg"
    }
  }
}
